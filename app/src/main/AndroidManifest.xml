<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.edwinpaezalonso.visitando">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="@string/google_maps_key" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />

            </intent-filter>
        </activity>

        <activity
            android:name="com.edwinpaezalonso.visitando.RegistroActivity"
            android:label="@string/app_name"
            android:parentActivityName="com.edwinpaezalonso.visitando.MainActivity"
            android:theme="@style/AppTheme" >
        </activity>

        <activity
            android:name="com.edwinpaezalonso.visitando.MapsActivity"
            android:label="@string/app_name"
            android:parentActivityName="com.edwinpaezalonso.visitando.MainActivity"
            android:theme="@style/AppTheme" >
        </activity>

        <activity
            android:name="com.edwinpaezalonso.visitando.ResetPasswordActivity"
            android:label="@string/app_name"
            android:parentActivityName="com.edwinpaezalonso.visitando.RegistroActivity"
            android:theme="@style/AppTheme" >
        </activity>


        <activity
            android:name="com.edwinpaezalonso.visitando.vista.ListaUnoActivity"
            android:label="@string/app_name"
            android:parentActivityName="com.edwinpaezalonso.visitando.RegistroActivity"
            android:theme="@style/AppTheme" >
        </activity>

        <activity
            android:name="com.edwinpaezalonso.visitando.vista.CatedralActivity"
            android:label="@string/app_name"
            android:parentActivityName="com.edwinpaezalonso.visitando.vista.ListaUnoActivity"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name="com.edwinpaezalonso.visitando.vista.ClubUnionActivity"
            android:label="@string/app_name"
            android:parentActivityName="com.edwinpaezalonso.visitando.vista.ListaUnoActivity"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name="com.edwinpaezalonso.visitando.vista.ParqueJegActivity"
            android:label="@string/app_name"
            android:parentActivityName="com.edwinpaezalonso.visitando.vista.ListaUnoActivity"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name="com.edwinpaezalonso.visitando.vista.LocomotoraActivity"
            android:label="@string/app_name"
            android:parentActivityName="com.edwinpaezalonso.visitando.vista.ListaUnoActivity"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name="com.edwinpaezalonso.visitando.vista.PuenteFerreoActivity"
            android:label="@string/app_name"
            android:parentActivityName="com.edwinpaezalonso.visitando.vista.ListaUnoActivity"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name="com.edwinpaezalonso.visitando.vista.EstacionActivity"
            android:label="@string/app_name"
            android:parentActivityName="com.edwinpaezalonso.visitando.vista.ListaUnoActivity"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name="com.edwinpaezalonso.visitando.vista.GranHotelActivity"
            android:label="@string/app_name"
            android:parentActivityName="com.edwinpaezalonso.visitando.vista.ListaUnoActivity"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name="com.edwinpaezalonso.visitando.vista.CamellonActivity"
            android:label="@string/app_name"
            android:parentActivityName="com.edwinpaezalonso.visitando.vista.ListaUnoActivity"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name="com.edwinpaezalonso.visitando.vista.ParqueBolivarActivity"
            android:label="@string/app_name"
            android:parentActivityName="com.edwinpaezalonso.visitando.vista.ListaUnoActivity"
            android:theme="@style/AppTheme" >
        </activity>

    </application>
    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true"/>

</manifest>